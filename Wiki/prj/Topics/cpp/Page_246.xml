    <header>
      <para style="margin-bottom:2px; margin-left:4px;"><anchor id="hdr" styleclass="Heading1"></anchor><text style="font-family:Verdana; font-size:10pt; font-weight:bold; color:#000000;" translate="true">vector</text></para>
    </header>
    <para><table rowcount="1" colcount="1" style="width:100%; cell-padding:1px; cell-spacing:0px; page-break-inside:auto; border-width:0px; border-spacing:0px; border-collapse:collapse; cell-border-width:0px; border-style:none; background-color:#99ccff; head-row-background-color:none; alt-row-background-color:none;">
      <tr style="vertical-align:top">
        <td style="vertical-align:middle;">
          <para><anchor id="hdr" style="font-family:Arial; font-size:10pt; color:#000000;"></anchor><text style="font-family:Verdana; font-size:8pt; font-style:italic; color:#000000;" translate="true">C++ Бъерн Страуструп</text></para>
        </td>
      </tr>
    </table></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">#include</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&#32;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#a31515;" translate="true">&lt;vector&gt;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">using</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&#32;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">namespace</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> std;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&#32;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">void</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> main()</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">{</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;vector&lt;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">int</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&gt; a(20, 3); </text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Создаем вектор с 20 элементами и инициализируем тройками</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//vector&lt;int&gt; a(20); //Создаем вектор с 20 элементами</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//vector&lt;int&gt; a; &#160; //Создаем пустой вектор</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;a.push_back(1); </text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Вставляю в конец</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;a.push_back(2); </text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Вставляю в конец</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;a.push_back(3); </text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Вставляю в конец</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&#32;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;a[0] = 12; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Пользуюсь как будто</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;a[1] = 13; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">// &#160;массивом</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&#32;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;a.pop_back(); &#160; </text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Удаляю последний элемент</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&#32;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;a.insert( (a.begin()+1), 5, 1); </text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Вставляю начиная с первого(не нулевого) 5 элементов равных 1</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">// &#160;вставляю между нулевым и первым </text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&#32;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">typedef</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> vector&lt;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">int</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&gt;::iterator typiter; </text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Альтернотивное имя</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&#32;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Цикл по всем элементам начиная с первого</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">for</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">( vector&lt;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">int</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&gt;::iterator iter = a.begin(); iter != a.end(); iter++ )</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;{</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160; &#160; &#160;*iter = 0;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160; &#160; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//iter[6] = 3; &#160;//К итератору можно обращаться как к массиву</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;}</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Цикл по всем элементам начиная с последнего</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">for</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">( vector&lt;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">int</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&gt;::reverse_iterator ri = a.rbegin(); ri != a.rend(); ri++ )</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;{</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160; &#160; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">static</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&#32;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">int</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> i=0;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160; &#160; &#160;*ri = i;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160; &#160; &#160;i++;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;}</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&#32;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Итераторы на константы</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;vector&lt;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">int</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&gt;::const_iterator &#160; &#160; &#160; &#160; cit &#160;= a.begin() ;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;vector&lt;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">int</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&gt;::const_reverse_iterator crit = a.rbegin();</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&#32;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Получаем количество элементов вектора</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#0000ff;" translate="true">int</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> s = a.size();</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&#32;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;a.erase(a.begin(), a.begin()+2); &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Удаляем с 0го по 1 элементы</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;a.erase(a.begin()+2); &#160; &#160; &#160; &#160; &#160; &#160; </text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Удаляем 2й элемент</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;</text><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#008000;" translate="true">//Удаляю все элементы вектора</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true"> &#160; &#160;a.clear();</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&#32;</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">}</text></para>
    <para style="margin-left:79px;"><text styleclass="Normal" style="font-family:Consolas; text-decoration:none; vertical-align:baseline; color:#000000;" translate="true">&#32;</text></para>
